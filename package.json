{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky install",
    "git:commit": "cz",
    "git:push": "git push -u origin HEAD",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier -w .",
    "find:unused": "next-unused",
    "postbuild": "next-sitemap --config next-sitemap.config.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/carousel": "^6.0.16",
    "@mantine/core": "^6.0.16",
    "@mantine/dates": "^6.0.16",
    "@mantine/dropzone": "^6.0.16",
    "@mantine/form": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@mantine/modals": "^6.0.16",
    "@mantine/notifications": "^6.0.16",
    "@mantine/nprogress": "^6.0.16",
    "@mantine/spotlight": "^6.0.16",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.9",
    "embla-carousel-react": "^8.0.0-rc11",
    "lucide-react": "0.105.0-alpha.4",
    "next": "^13.4.8",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.31.3",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@storybook/cli": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql": "^16.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "next-sitemap": "^4.1.8",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "storybook-addon-turbo-build": "^2.0.1",
    "storybook-dark-mode": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.js"
    }
  }
}
